{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Attach to Docker",
        "type": "node",
        "request": "attach",
        "address": "localhost",
        "port": 9229,
        "localRoot": "${workspaceFolder}",
        "remoteRoot": "/usr/src/app",
        "skipFiles": [
          "<node_internals>/**/*.js",
          "${workspaceFolder}/node_modules/**/*"
        ]
      }
    ]
  }
  